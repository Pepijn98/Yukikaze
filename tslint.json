{
    "defaultSeverity": "error",
    "extends": "gts/tslint.json",
    "jsRules": {},
    "rules": {
        "import-name": false,
        "quotemark": [
            true,
            "double",
            "avoid-escape",
            "avoid-template"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "trailing-comma": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}
